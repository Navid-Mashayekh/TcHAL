    <Method Name="set_value" Id="{d5d61d95-3742-077a-3f8e-b7ed44bf5f79}">
      <Declaration><![CDATA[METHOD set_value : BOOL
VAR_INPUT
	value : Tc2_Utilities.T_Arg ;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF value.eType=THIS^.PropertyType THEN
    _valueToWrite := value;
    IF THIS^._I_CoE_communication_handler <> 0 THEN 
        THIS^._I_CoE_communication_handler.add_write_request(THIS^);
        set_value := TRUE;
    ELSE
        set_value := FALSE;
        errId:=1;
    END_IF
ELSE
    set_value := FALSE;
    errId := 400;
END_IF
]]></ST>
      </Implementation>
    </Method>
