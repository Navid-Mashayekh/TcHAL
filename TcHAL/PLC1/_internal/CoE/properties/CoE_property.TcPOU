  <POU Name="CoE_property" Id="{7290e4d7-edb9-0644-02c0-672d2dd6d3bf}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK CoE_property IMPLEMENTS I_CoE_property
VAR_INPUT
END_VAR
VAR_OUTPUT
END_VAR
VAR
	_SubIndex 		: BYTE;
	_Index			: WORD;
    _propertyType : Tc2_Utilities.E_ArgType := Tc2_Utilities.ARGTYPE_UNKNOWN;
	_init_done		: BOOL;
	_neg_replay		: f_trig;
	_SlaveAddress	: WORD;
	_propertyPullMode	: UINT;
	_pollingInterval: TIME;
	_I_CoE_communication_handler: I_CoE_communication_handler;
	
	_read_errId		: UDINT;
	_write_errId	: UDINT;
	_init_state		: INT;
	
	_valueToWrite 	: Tc2_Utilities.T_Arg;
    _value : Tc2_Utilities.T_Arg;

	pollingTimer: ton;
	
	errId: INT;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[CASE _propertyPullMode OF 
	0:  // cashed 
		;
	
	1:  // polling 
		pollingTimer(IN:=NOT pollingTimer.q, PT:=_pollingInterval);
		IF pollingTimer.Q THEN
			IF THIS^._I_CoE_communication_handler <> 0 THEN 
				THIS^._I_CoE_communication_handler.add_read_request(THIS^);
			ELSE
				errId:=1;
			END_IF
		END_IF
	
	2: // on request
		IF THIS^._I_CoE_communication_handler <> 0 THEN 
			THIS^._I_CoE_communication_handler.add_read_request(THIS^);
		ELSE
			errId:=1; 
		END_IF

ELSE
	errId:=2; 
END_CASE]]></ST>
    </Implementation>
