    <Method Name="get_value" Id="{284cb2f0-d566-0231-18a9-fcedb75b024a}">
      <Declaration><![CDATA[METHOD get_value : BOOL
VAR_OUTPUT
	value : Tc2_Utilities.T_Arg ;
	errId : UDINT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF init_done THEN
        IF _value.eType=THIS^.PropertyType THEN
        value := _value;
        get_value := TRUE;
    ELSE
        errId := 400;
        get_value := FALSE;
	END_IF
ELSE
	errId := 2;
	get_value := FALSE;
END_IF
]]></ST>
      </Implementation>
    </Method>
