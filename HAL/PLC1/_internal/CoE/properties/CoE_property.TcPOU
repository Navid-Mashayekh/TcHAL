    <Method Name="reinit" Id="{30436167-4235-0089-245f-f4e6c137a925}">
      <Declaration><![CDATA[METHOD reinit : BOOL
VAR_OUTPUT
	errId: UDINT;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[
CASE _init_state OF 
	0:  // add read request and continue
		_init_done:=FALSE;
		reinit:=FALSE;
		IF THIS^._CoE_property_handler <> 0 THEN 
			THIS^._CoE_property_handler.add_read_request(THIS^);
		ELSE
			errId:=1;
		END_IF
		_init_state := 10;
		
	10: // wait for read callback
		reinit:=TRUE;
		errId:=_read_errId;
		_init_state := 0;
END_CASE]]></ST>
      </Implementation>
    </Method>
